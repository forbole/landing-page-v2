{
  "presets": [
    [
      "next/babel"
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "@modules": "modules",
          "@components": "./components",
          "@screens": "./screens",
          "@src": "./"
        }
      }
    ]
  ],
  "env": {
    "test": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "modules": false
          }
        ],
        "next/babel"
      ]
    }
  }
}
